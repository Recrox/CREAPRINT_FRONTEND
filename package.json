{
  "name": "creaprint-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc && node ./scripts/copy-assets.js",
  "dev:build": "tsc --watch --preserveWatchOutput",
  "dev:serve": "lite-server --baseDir=dist",
  "dev:bundle": "esbuild --bundle dist/main.js --outfile=dist/bundle.js --platform=browser --format=esm --sourcemap --watch",
  "dev": "npm run clean && node ./scripts/copy-assets.js && concurrently \"npm:dev:build\" \"npm:dev:bundle\" \"npm:dev:serve\"",
    "start": "npm run dev",
    "nswag:generate": "npx nswag run ./nswag.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "lite-server": "^2.6.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.1.6",
    "esbuild": "^0.18.0",
    "concurrently": "^8.2.0"
  }
  ,
  "dependencies": {
    "@angular/animations": "20.0.0",
    "@angular/common": "20.0.0",
    "@angular/compiler": "20.0.0",
    "@angular/core": "20.0.0",
    "@angular/forms": "20.0.0",
    "@angular/platform-browser": "20.0.0",
    "@angular/platform-browser-dynamic": "20.0.0",
    "@angular/router": "20.0.0",
    "@angular/material": "20.0.0",
    "@angular/cdk": "20.0.0",
    "tslib": "^2.5.0",
    "rxjs": "^7.8.0",
    "zone.js": "~0.15.1",
    "axios": "^1.5.0"
  }
}
